{
  "name": "@crawlee-cloud/api",
  "version": "0.1.0",
  "description": "Self-hosted Apify-compatible REST API server",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "clean": "rm -rf dist coverage",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "db:migrate": "tsx src/db/migrate.ts"
  },
  "keywords": [
    "crawlee",
    "apify",
    "api",
    "self-hosted"
  ],
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.700.0",
    "@fastify/cors": "^10.0.0",
    "@fastify/websocket": "^11.0.1",
    "bcryptjs": "^2.4.3",
    "fastify": "^5.1.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "nanoid": "^3.3.11",
    "pg": "^8.13.1",
    "uuid": "^11.0.3",
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.11.10",
    "@types/uuid": "^10.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
